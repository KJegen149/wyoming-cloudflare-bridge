{
  "name": "wyoming-cloudflare-workers",
  "version": "1.0.0",
  "description": "Cloudflare Workers for Wyoming Protocol STT and TTS",
  "scripts": {
    "deploy-stt": "wrangler deploy --config wrangler-stt.toml",
    "deploy-tts": "wrangler deploy --config wrangler-tts.toml",
    "deploy-all": "npm run deploy-stt && npm run deploy-tts",
    "tail-stt": "wrangler tail wyoming-stt",
    "tail-tts": "wrangler tail wyoming-tts"
  },
  "keywords": [
    "wyoming",
    "home-assistant",
    "voice-assistant",
    "cloudflare",
    "workers-ai"
  ],
  "author": "",
  "license": "MIT"
}
